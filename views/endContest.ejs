<%include header %>

<div style="position:absolute;left:500px;top:75px">
   	<h2><u>Delete From Upcoming!</u></h2>
</div>
<div id = "contestCred" style = "display:block;position:absolute;left:400px;top:200px">
<div class="form-group">

		<label for = "id">Enter Contest Id!</label>
		<input type = "text" class = "form-control" id = "id" size = "10px">
		<p id = "info1" style = "color:Red">Enter a Valid Id!</p>
		<div class="form-group">
            <div class="col-sm-offset-2 col-sm-10">
                <button id = "submit1" class="btn btn-default">Delete from Upcoming!</button>
            </div>
        </div>
        <p id = "res"></p>
</div>
</div>
</body>
<script src="/socket.io/socket.io.js"></script>
<script>
	var nsp = io.connect('/admin');
	var x = document.getElementById('submit1');
	x.addEventListener('click', function(){
		var value = document.getElementById('id').value;
		console.log(value);
		nsp.emit('delUpcoming', value);
	});
	socket.on('deleted', function (data){
		document.getElementById('res').innerHTML = '<h2>' + data + '</h2>';
	});
</script>
